defaults: &defaults
  twilio:
    account_sid: <%= ENV['TWILIO_ACCOUNT_SID'] %>
    auth_token: <%= ENV['TWILIO_AUTH_TOKEN'] %>
    app_sid: <%= ENV['TWILIO_APP_SID'] %>
  iron:
    project_id: <%= ENV['IRON_WORKER_PROJECT_ID'] %>
    token: <%= ENV['IRON_WORKER_TOKEN'] %>

production:
  <<: *defaults
